{
  "name": "react-responsive-bricks",
  "description": "A responsive masonry layout for React",
  "homepage": "https://mickybizzle.github.io/react-responsive-bricks/",
  "version": "0.1.3",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MickyBizzle/react-responsive-bricks.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish:npm": "SET NODE_ENV=production & rmdir /s /q dist 2> nul & mkdir dist & npx babel src/Bricks --out-dir dist --copy-files",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1"
  }
}
